    "globals": {
      "chrome": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    },
    "rules": {
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "net-browserify": "^0.2.4",
    "openrgb-sdk": "^0.5.2",
    "yargs": "^17.3.1"
  },
  "scripts": {
    "build": "gulp babel",
    "proxy": "node proxy/server.js",
    "server": "concurrently --kill-others \"npm run proxy\" \"openrgb --server\""
  }
